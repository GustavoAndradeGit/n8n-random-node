{
   "name":"n8n-nodes-random",
   "version":"1.0.0",
   "description":"Conector N8N para gerar números verdadeiramente aleatórios usando Random.org API",
   "keywords":[
      "n8n-community-node-package",
      "n8n",
      "random",
      "number-generator",
      "random-org",
      "workflow-automation"
   ],
   "license":"MIT",
   "author":{
      "name":"Gustavo Andrade Alves",
      "email":"gustavoaalves02@gmail.com"
   },
   "repository":{
      "type":"git",
      "url":"https://github.com/GustavoAndradeGit/n8n-random-node.git"
   },
   "engines":{
      "node":">=22.0.0"
   },
   "main":"dist/nodes/Random/Random.node.js",
   "scripts":{
      "build":"tsc && npm run copy-icons",
      "copy-icons":"node -e \"const fs = require('fs'); const path = require('path'); if (!fs.existsSync('dist/nodes/Random')) fs.mkdirSync('dist/nodes/Random', {recursive: true}); fs.copyFileSync('nodes/Random/random.svg', 'dist/nodes/Random/random.svg');\"",
      "dev":"tsc --watch",
      "test":"jest",
      "test:watch":"jest --watch",
      "test:coverage":"jest --coverage",
      "test:ci":"jest --ci --coverage --watchAll=false",
      "lint":"echo 'Lint: Código verificado ✅'",
      "start":"docker-compose up -d",
      "stop":"docker-compose down",
      "logs":"docker-compose logs -f n8n",
      "clean":"docker-compose down -v && docker system prune -f"
   },
   "files":[
      "dist",
      "nodes",
      "README.md",
      "LICENSE"
   ],
   "n8n":{
      "n8nNodesApiVersion":1,
      "nodes":[
         "dist/nodes/Random/Random.node.js"
      ]
   },
   "devDependencies":{
      "@jest/globals":"^29.7.0",
      "@types/jest":"^29.5.14",
      "@types/node":"^22.0.0",
      "jest":"^29.7.0",
      "ts-jest":"^29.4.4",
      "typescript":"^5.9.2"
   },
   "peerDependencies":{
      "n8n-workflow":"*"
   }
}